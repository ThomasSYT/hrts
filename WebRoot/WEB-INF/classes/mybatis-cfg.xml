<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
   <settings>
   <!-- 全局启用或禁用延迟加载。当禁用时，所有关联对象都会即时加载。默认：true -->
   <setting name="lazyLoadingEnabled" value="true"/>

   <!-- 当启用时，有延迟加载属性的对象在被调用时将会完全加载任意属性。否则，每种属性将会按需要加载 -->
   <setting name="aggressiveLazyLoading" value="false"/>
 </settings>

 <typeAliases>
 	
 	<typeAlias type="com.examngfinal.bean.sys.SysFun" alias="sysfun"/>
 	<typeAlias type="com.examngfinal.bean.sys.SysUser" alias="user"/>
 	<typeAlias type="com.examngfinal.bean.sys.Employee" alias="employee"/>
 	<typeAlias type="com.examngfinal.bean.sys.SysRole" alias="role"/>
 	<typeAlias type="com.examngfinal.bean.sys.Department" alias="department"/>
 	<typeAlias type="com.examngfinal.bean.job.JobTask" alias="jobTask"/>
 	<typeAlias type="com.examngfinal.bean.job.JobLog" alias="jobLog"/>
 	<typeAlias type="com.examngfinal.bean.job.JobLeave" alias="jobLeave"/>
 	<typeAlias type="com.examngfinal.bean.job.JobLearn" alias="jobLearn"/>
 	<typeAlias type="com.examngfinal.bean.job.JobExam" alias="jobExam"/>
 	<typeAlias type="com.examngfinal.bean.job.Complain" alias="complain"/>
 	<typeAlias type="com.examngfinal.bean.Improvement" alias="improvement"/>
 	<typeAlias type="com.examngfinal.bean.vo.AbilityVo" alias="abilityVo"/>
 	<typeAlias type="com.examngfinal.bean.TbPay" alias="tbPay"/>
 </typeAliases>


<mappers>
    <!--mapper对应的xml配置文件-->
    
    <mapper resource="com/examngfinal/dao/SysFun.xml" />
    <mapper resource="com/examngfinal/dao/SysUser.xml" />
    <mapper resource="com/examngfinal/dao/JobTask.xml" />
    <mapper resource="com/examngfinal/dao/JobLog.xml" />
    <mapper resource="com/examngfinal/dao/JobLe.xml" />
    <mapper resource="com/examngfinal/dao/JobExam.xml" />
    <mapper resource="com/examngfinal/dao/SysRole.xml" />
    <mapper resource="com/examngfinal/dao/improvement.xml" />
    <mapper resource="com/examngfinal/dao/PayController.xml" />
</mappers>
</configuration>
